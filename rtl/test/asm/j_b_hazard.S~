# test Jump & Branch & Load & Store format instruction with data hazard 

addi x26,x0,1025     # x26 = 1025
addi x27,x0,2        # x27 = 2

beq  x26,x27,8       # if x26 = x27, jump to insn(jal x0,16)
addi x27,x26,4       # x27 = x26+4  (x26 data hazard)

jal  x0,16           # jump to insn (sw   x28,x26,1028)

sb   x26,1024(x27)   # x26 --> [x27 + 1024] 
sh   x27,1028(x26)   # x27 --> [x26 + 1028]
lb   x28,1024(x27)   # [x27 + 1024] byte --> x28  

#lh   x29,1028(x26)   # [x26 + 1028] half_word --> x29  

sw   x26,1028(x26)   # x26 --> [x26 + 1028] = 1025 --> [2053]
lbu  x27,1028(x26)   # [x26 + 1028] --> x27 = 1     (load hazard)
lhu  x30,1028(x26)   # [x26 + 1028] --> x30 = 1025  (load hazard)
add  x29,x26,x27     # x29 = x26 + x27 = 1026     (x27 data hazard)
addi x28,x0,14       # x28 = 14

add  x30,x26,x28     # x30 = x26 + x28 = 1039     (x28 data hazard)
add  x27,x27,x29     # x27 = x27 + x29 = 1027     (x29 data hazard)
bne  x27,x30,8       # if x26 != x30,jump to insn(add  x27,x27,x29) 

lw   x27,1028(x26)   # [x26 + 1028] --> x27    
blt  x29,x28,8       # if x29 < x28, jump to insn(add  x27,x27,x29) 

add  x27,x27,x29     # x27 = x27 + x29 = 2053
bge  x29,x28,8       # if x29 > x28, jump to insn(bltu x29,x28,8) 

add  x27,x27,x29     # x27 = x27 + x29
add  x27,x27,x29     # x27 = x27 + x29 

bltu x29,x28,8       # if x29 < x28, jump to insn(add  x30,x26,x28)  
bgeu x29,x28,8       # if x29 > x28, jump to insn(add  x27,x27,x29)
add  x30,x26,x28     # x30 = x26 + x28 

add  x30,x26,x28     # x30 = x26 + x28 

add  x27,x27,x29     # x27 = x27 + x29 = 3079
